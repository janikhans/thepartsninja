<div id="id_<%= compatible.id %>" class="vote-item">
    <div class="row">
        <div class="col-md-12">
            <div class="vote-actions">
                <a href="#">
                    <i class="fa fa-chevron-up"> </i>
                </a>
                <div>34</div>
                <a href="#">
                    <i class="fa fa-chevron-down"> </i>
                </a>
            </div>
            <div class="vote-title">
                <%= compatible.compatible_part.product.brand.name %> <%= compatible.compatible_part.product.name %> 
            </div>
            <div class="vote-info">
            Designed For: <% compatible.compatible_part.fitments.each do |f| %> <%= f.vehicle.year %> <%= f.vehicle.brand.name %> <%= f.vehicle.model %> <% end %>
                <div class="col-md-4"><i class="fa fa-comments-o"></i> Replaces: <%= compatible.part.product.name %></div>
                <div class="col-md-4"><i class="fa fa-clock-o"></i> Modifications: <%= compatible.discovery.modifications %></div>
                <div class="col-md-4"><i class="fa fa-gears"></i><%= link_to " View Discovery", compatible.discovery  %></div> 
            </div>
        </div>
    </div>
</div>
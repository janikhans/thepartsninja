<% content_for :header_topic do %>
  Compatibility Check
<% end %>
<% content_for :header do %>
  <%= render 'layouts/header_partials/default' %>
  <%= render 'layouts/header_partials/quicklinks' %>
<% end %>
<div class="row">
  <div class="col-md-9">
    <div id= "compatibility-check-form" class="m-b-md">
      <div class="topic-title">
        <h3>Check Compatibility</h3>
      </div>
      <%= render 'form' %>
    </div>
    <div class="topic-title">
      <h3>Results</h3>
    </div>
    <div id="compatibility-check-results">
      <%= render 'results' %>
    </div>
  </div>
  <div class="col-md-3">
    <%= render 'shared/profile' %>
    <%= render 'shared/ebay_results_sidebar' %>
  </div>
</div>

<% if @search.errors.any? %>
  $('#search-stats').html("<%= escape_javascript(render 'shared/error_messages', object: @search) %>");
<% elsif @check_search.category_invalid? %>
  $('#search-stats').html("<%= escape_javascript(render partial: 'category_invalid', locals: { search_record: @check_search } ) %>");
<% elsif @check_search.successful? %>
  $('#search-stats').html("<%= escape_javascript(render partial: 'successful', locals: { search_record: @check_search } ) %>");
<% else %>
  $('#search-stats').html("<%= escape_javascript(render partial: 'unsuccessful', locals: { search_record: @check_search } ) %>");
<% end %>
$('#results').empty();

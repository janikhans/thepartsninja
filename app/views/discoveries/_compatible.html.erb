<div class="vote-item compatible">
  <div class="row">
    <div class="col-md-1 col-xs-1">
        <div class="vote-actions">
          <%= vote_actions(compatible) %>
        </div>
    </div>
    <div class="col-md-5 col-xs-5 text-left">
        <div class="part">
          <%= compatible.part.product.brand.name %> <%= compatible.part.product.name %>
        </div>
        <div class="part-info">
            <%= link_to('Vehicles +', '#', 'data-toggle': "popover", title: "Built for...", 'data-placement': "bottom", 'data-html': true, 'data-content': "#{vehicle_list(compatible.part)}") %>
            PN: <%= link_to "#{compatible.part.part_number}", compatible.part %>
        </div>
    </div>
    <div class="col-md-1 col-xs-1">
        <div class="backwards">
          <%= backwards(compatible) %>
        </div>
    </div>
    <div class="col-md-5 col-xs-5 text-right">
        <div class="part">
          <%= compatible.compatible_part.product.brand.name %> <%= compatible.compatible_part.product.name %>
        </div>
        <div class="part-info">
            <%= link_to('Vehicles +', '#', 'data-toggle': "popover", title: "Built for...", 'data-placement': "bottom", 'data-html': true, 'data-content': "#{vehicle_list(compatible.compatible_part)}") %>
            PN: <%= link_to "#{compatible.compatible_part.part_number}", compatible.compatible_part %>
        </div>
    </div>
  </div>
</div>

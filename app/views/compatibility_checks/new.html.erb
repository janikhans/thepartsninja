<% content_for :header_topic do %>
  Compatibility Check Tool
<% end %>
<% content_for :header do %>
  <%= render 'layouts/header_partials/default' %>
  <%= render 'layouts/header_partials/quicklinks' %>
<% end %>
<div class="row">
  <div id="compatibility-check-form" class="col-md-12">
    <h3>Begin Your Search</h3>
    <div class="ibox-content shadow">
      <%= render 'compatibility_checks/form' %>
    </div>
  </div>
</div>
<div class="row m-t-sm">
  <div id="compatibility-check" class="col-md-9">
    <%= render 'results' %>
  </div>
  <div id="ebay-results-sidebar" class="col-md-3">
    <%= render 'layouts/ebay_results_sidebar' %>
  </div>
</div>

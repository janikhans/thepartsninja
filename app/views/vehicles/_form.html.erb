<%= form_for(@vehicle) do |f| %>
  <div class="field">
    <%= f.label :model %><br>
    <%= f.text_field :model %>
  </div>
  <div class="field">
    <%= f.label :year %><br>
    <%= f.number_field :year %>
  </div>
  <div class="field">
    <%= f.label :brand_name %><br>
    <%= f.text_field :brand_name, data: {autocomplete_source: autocomplete_brands_path} %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
  <% end %>
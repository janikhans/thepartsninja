<tr id="<%= vehicle.id %>">
  <td><%= vehicle.oem_parts.count %></td>
  <td><%= vehicle.id %></td>
  <td><%= vehicle.year %></td>
  <td><%= vehicle.brand.name %></td>
  <td><%= vehicle.model %></td>
  <td><%= link_to 'View', vehicle, class: "btn nj-btn btn-thin" %> <%= link_to 'Delete', vehicle, method: :delete, data: {confirm: "Are you sure"}, class: "btn nj-btn btn-thin" %></td>
</tr>
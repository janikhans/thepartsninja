<tr>
  <td><%= lead.email %></td>
  <td><%= bool_to_checkmark(lead.auto) %></td>
  <td><%= bool_to_checkmark(lead.dirtbike) %></td>
  <td><%= bool_to_checkmark(lead.streetbike) %></td>
  <td><%= bool_to_checkmark(lead.atv) %></td>
  <td><%= bool_to_checkmark(lead.utv) %></td>
  <td><%= bool_to_checkmark(lead.dualsport) %></td>
  <td><%= bool_to_checkmark(lead.snowmobile) %></td>
  <td><%= bool_to_checkmark(lead.watercraft) %></td>
  <td><%= lead.created_at.strftime("%b %d, %Y") %></td>
  <td><%= link_to "Delete", admin_lead_path(lead), method: :delete, data: {confirm: "Are you sure?"}  %></td>
</tr>

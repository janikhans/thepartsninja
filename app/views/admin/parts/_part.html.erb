<tr>
  <td><%= part.product.brand.name %></td>
  <td><%= part.product.ebay_category ? part.product.ebay_category.name : nil %><%= part.product.category.try(:name) %></td>
  <td><%= part.product.name %></td>
  <td><%= part.part_number %></td>
  <td><%= part.note %></td>
  <td><% part.part_attributes.each do |p| %><%= p.parent_attribute.name %>: <%= p.name %> <% end %></td>
  <td><%= link_to 'View', admin_part_path(part) %></td>
  <td><%= link_to 'Edit', edit_admin_part_path(part) %></td>
</tr>

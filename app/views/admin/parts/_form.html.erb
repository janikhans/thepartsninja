<%= simple_form_for([:admin, @part]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :part_number %>
    <%= f.input :note %>
    <%= f.association :product, label_method: :to_label %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
